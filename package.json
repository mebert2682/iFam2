  "dependencies": {
    "axios": "^0.19.0",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.12",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
